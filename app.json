{
  "expo": {
    "name": "App Map Draggable",
    "slug": "app-map-draggable",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.draggable"
    },
    "android": {
      "package": "br.com.draggable",
      "permissions": [
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE",
        "MANAGE_DOCUMENTS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBlAEYnj-SO1yS14jABE4iH4BgvZj3PFoA"
        }
      }
    }
  }
}